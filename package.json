{
  "name": "rollup-build-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Acongm/rollup.git",
  "author": "acongm <acongm@126.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup ./src/main.js --file ./dist/index.js --format cjs",
    "build:es": "rollup ./src/main.js --file ./dist/index.es.js --format es",
    "build:umd": "rollup ./src/main.js --file ./dist/index.mjs --format umd --name 'myBundle'"
  },
  "devDependencies": {
    "rollup": "^3.3.0"
  }
}
